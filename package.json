{
  "name": "browser",
  "version": "0.0.1",
  "license": "GPLV3",
  "repository": "https://github.com/gorinje/browser",
  "main": "electron/main.js",
  "author": "UniLaSalle",
  "description": "Version d√©veloppement d'un squelette de navigateur ",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p serve start:electron",
    "build": "ng build",
    "serve": "ng serve",
    "start:electron": "wait-on tcp:4200 && electron . --serve",
    "build:mac": "ng build --base-href ./ && electron-builder -m --publish=never",
    "build:win": "ng build --base-href ./ && electron-builder -w --publish=never",
    "build:linux": "ng build --base-href ./ && electron-builder -l --publish=never",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.0",
    "@angular/common": "^16.1.0",
    "@angular/compiler": "^16.1.0",
    "@angular/core": "^16.1.0",
    "@angular/forms": "^16.1.0",
    "@angular/platform-browser": "^16.1.0",
    "@angular/platform-browser-dynamic": "^16.1.0",
    "@angular/router": "^16.1.0",
    "@cosmograph/cosmos": "^1.4.1",
    "@fortawesome/angular-fontawesome": "^0.13.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@memgraph/orb": "^0.4.1",
    "d3": "^7.8.5",
    "electron-is-dev": "^2.0.0",
    "highcharts": "^11.3.0",
    "highcharts-angular": "^4.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "vis-network": "^9.1.9",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.1.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "^16.1.0",
    "@types/cytoscape": "^3.19.16",
    "@types/d3": "^7.4.3",
    "@types/jasmine": "~4.3.0",
    "@types/leaflet": "^1.9.8",
    "electron": "^27.0.2",
    "electron-builder": "^24.6.4",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.3",
    "wait-on": "^7.1.0"
  },
  "build": {
    "directories": {
      "output": "releases"
    },
    "files": [
      "electron/**/*",
      "dist/**/*"
    ]
  }
}
